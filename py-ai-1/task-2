import random

low = int(input("Введіть нижню межу: "))
high = int(input("Введіть верхню межу: "))

matrix = [[random.randint(low, high) for _ in range(5)] for _ in range(5)]

print("Матриця:")
for row in matrix:
    print(row)

# Головна діагональ
diagonal = [matrix[i][i] for i in range(5)]
print("Головна діагональ:", diagonal)
print("Сума діагоналі:", sum(diagonal))


for i in range(5):
    for j in range(i + 1, 5):
        matrix[i][j] = 0

print("Змінена матриця:")
for row in matrix:
    print(row)
