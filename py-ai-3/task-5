import matplotlib.pyplot as plt

hours = list(range(24))
load = [20, 18, 15, 14, 13, 15, 25, 40, 60, 70, 80, 85,
        90, 88, 85, 80, 75, 70, 65, 55, 45, 35, 30, 25]

plt.plot(hours, load)
plt.fill_between(hours, load)
plt.xlabel("Години")
plt.ylabel("Навантаження (%)")
plt.title("Навантаження сервера за добу")
plt.grid()
plt.show()
