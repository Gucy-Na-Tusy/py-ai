math = [80, 92, 75, 60, 88]
physics = [70, 65, 90, 85, 78]
english = [95, 100, 85, 90, 87]

subjects = {
    "Math": math,
    "Physics": physics,
    "English": english
}

stats = {}

for subject, scores in subjects.items():
    stats[subject] = {
        "average": sum(scores) / len(scores),
        "min": min(scores),
        "max": max(scores)
    }

best_subject = max(stats, key=lambda x: stats[x]["average"])

stats, best_subject

